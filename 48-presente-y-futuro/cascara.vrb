\frametitle{Temporal query}

\includegraphics{temporal.jpg}

\begin{verbatim}
ALTER TABLE variants
ADD CONSTRAINT tfk_variants_product_id
FOREIGN KEY (product_id, PERIOD valid_at)
REFERENCES (id, PERIOD valid_at);
\end{verbatim}

